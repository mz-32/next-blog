    "next-optimized-images": "^2.5.4",
    "node-sass": "^4.13.0",
    "path-match": "^1.2.4",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
