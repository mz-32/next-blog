    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "jest": "^24.9.0",
    "processmd": "^4.5.0",
